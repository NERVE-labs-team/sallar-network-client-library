{
  "name": "@sallar-network/client",
  "version": "1.0.0",
  "description": "Server-side library for manage and communicate with programs running on the sallar network",
  "main": "dist/index.global.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "generate-docs": "npx typedoc src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "socket.io-client": "^4.8.1",
    "tsup": "^8.4.0",
    "typedoc": "^0.28.3",
    "typescript": "^5.8.3"
  }
}
